{
  "name": "mattermost-client-node",
  "version": "7.2.0",
  "description": "Mattermost client",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "scripts": {
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint src --ext .js,.ts",
    "test:unit": "jest --runInBand",
    "test:unit:watch": "jest --runInBand --watch",
    "build": "rollup -c",
    "docs": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitExpert/mattermost-client-node.git"
  },
  "keywords": [
    "mattermost",
    "mattermost-client",
    "client",
    "node",
    "nodejs",
    "messaging",
    "collaboration",
    "teams",
    "communications",
    "javascript",
    "typescript",
    "ts",
    "js",
    "chat"
  ],
  "author": "Daniel Ruf <daniel@daniel-ruf.de>",
  "contributors": [
    {
      "name": "Hagen Morano",
      "email": "mail@hagenmorano.de"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitExpert/mattermost-client-node/issues"
  },
  "homepage": "https://github.com/bitExpert/mattermost-client-node#readme",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "extend": "^3.0.2",
    "https-proxy-agent": "^4.0.0",
    "isomorphic-ws": "^4.0.1",
    "log": "^6.0.0",
    "request": "^2.88.0",
    "text-encoding": "^0.7.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/jest": "^24.9.0",
    "@types/request": "^2.48.4",
    "@types/text-encoding": "^0.0.35",
    "@types/ws": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": ">=10.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^24.3.0",
    "tslib": "^1.10.0",
    "typedoc": "^0.16.7",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint",
      "git add"
    ]
  }
}
